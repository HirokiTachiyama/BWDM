class MyersTrianglefunctionspublic 三角形 : int * int * int -> bool    三角形(A,B,C)==        let 周 = A + B + C in            周 > (2 * A) and 周 > (2 * B) and 周 > (2 * C);public 形状判断 : int * int * int -> seq of char    形状判断(A,B,C)==        if 三角形(A,B,C) then            if (A=B and B=C) then                "正三角形かつ二等辺三角形"            elseif (A=B or B=C or C=A) then                "二等辺三角形"            else                "不等辺三角形"        else            "非三角形";end MyersTriangle