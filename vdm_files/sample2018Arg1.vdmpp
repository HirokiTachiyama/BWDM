class sampleClass

functions

-- arg    a:int
-- return seq of char(string)
-- 戻り値の文字列には、その行が実行される際に引数aが満たす条件を示す
-- ツールの処理と文字列の内容は無関係である

sampleFunction : int -> seq of char
sampleFunction(a) ==
    if(a mod 100 = 0) then
      if(a > 2018) then
        "aは100で割り切れるかつ2018より大きい"
      elseif(a mod 13 = 0) then
        "aは100で割り切れるかつ2018未満かつ13で割り切れる"
      else
        "aは100で割り切れるかつ2018未満かつ13で割り切れない"
    else
      "aは100で割り切れない";

end sampleClass
