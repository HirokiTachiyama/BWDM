class Sample

functions

SampleFunction : int*nat -> seq of char
    SampleFunction(a, b) ==
       if(a <= 10) then
         if(-1 < a) then
      	   "-1<a<=10"
    	 else if(b > 5) then
    	   "a<=-1 & 5<b"
	 else
	   "a<=-1 & b<=5"
       else
         if( b < -3 ) then
           "10<a & b<-3"
         else if(a < 20) then
           "10<a<20 & -3<=b"
	 else
	   "20<=a & -3<=b";
end Sample
